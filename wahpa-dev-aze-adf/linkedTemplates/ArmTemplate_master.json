{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "wahpa-dev-aze-adf"
		},
		"ls_az_sql_login_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_az_sql_login'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=@{linkedService().az_sql_server_name};Initial Catalog=@{linkedService().az_sql_database_name};User ID=@{linkedService().az_sql_login_user_name}"
		},
		"ls_adls2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().adls2_endpoint}"
		},
		"ls_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "@{linkedService().key_vault_URI}"
		},
		"test-trigger_properties_pipeline2_parameters_folderPath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"test-trigger_properties_pipeline2_parameters_fileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_server_name": {
			"type": "string",
			"defaultValue": "wahpa-dev-aze-sql.database.windows.net"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_database_name": {
			"type": "string",
			"defaultValue": "SampleDB"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_login_user_name": {
			"type": "string",
			"defaultValue": "User51126629"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_login_password_key_vault_URI": {
			"type": "string",
			"defaultValue": "https://wahpa-dev-aze-kv.vault.azure.net/"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_login_password_kv_secret_name": {
			"type": "string",
			"defaultValue": "wahpa-dev-aze-sql-admin-password"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_schema_name": {
			"type": "string",
			"defaultValue": "dbo"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_table_name": {
			"type": "string",
			"defaultValue": "@split(triggerbody().fileName, '.')[0]"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_endpoint": {
			"type": "string",
			"defaultValue": "https://wahpadevazestraw.dfs.core.windows.net/"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_container_name": {
			"type": "string",
			"defaultValue": "@split(triggerbody().folderPath, '/')[0]"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_directory_name": {
			"type": "string",
			"defaultValue": "@split(triggerbody().folderPath, '/')[1]"
		},
		"test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_file_name": {
			"type": "string",
			"defaultValue": "@triggerbody().fileName"
		},
		"test-trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/687fe1ae-a520-4f86-b921-a80664c40f9b/resourceGroups/wahpa-dev-aze-rg-services/providers/Microsoft.Storage/storageAccounts/wahpadevazestraw"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "wahpa-dev-aze-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_az_sql_login_connectionString": {
						"value": "[parameters('ls_az_sql_login_connectionString')]"
					},
					"ls_adls2_properties_typeProperties_url": {
						"value": "[parameters('ls_adls2_properties_typeProperties_url')]"
					},
					"ls_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_properties_typeProperties_baseUrl')]"
					},
					"test-trigger_properties_pipeline2_parameters_folderPath": {
						"value": "[parameters('test-trigger_properties_pipeline2_parameters_folderPath')]"
					},
					"test-trigger_properties_pipeline2_parameters_fileName": {
						"value": "[parameters('test-trigger_properties_pipeline2_parameters_fileName')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_server_name": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_server_name')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_database_name": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_database_name')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_login_user_name": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_login_user_name')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_login_password_key_vault_URI": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_login_password_key_vault_URI')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_login_password_kv_secret_name": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_login_password_kv_secret_name')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_schema_name": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_schema_name')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_table_name": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_az_sql_table_name')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_endpoint": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_endpoint')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_container_name": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_container_name')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_directory_name": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_directory_name')]"
					},
					"test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_file_name": {
						"value": "[parameters('test-trigger_properties_adls2_delimited_text_to_sql_parameters_adls2_file_name')]"
					},
					"test-trigger_properties_typeProperties_scope": {
						"value": "[parameters('test-trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}