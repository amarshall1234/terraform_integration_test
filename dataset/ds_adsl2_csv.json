{
	"name": "ds_adsl2_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_adls2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"adls2_endpoint": {
				"type": "string"
			},
			"adls2_container_name": {
				"type": "string"
			},
			"adls2_directory_name": {
				"type": "string"
			},
			"adls2_file_name": {
				"type": "string"
			},
			"compression_type": {
				"type": "string",
				"defaultValue": "None"
			},
			"compression_level": {
				"type": "string",
				"defaultValue": "Optimal"
			},
			"column_delimiter": {
				"type": "string",
				"defaultValue": ","
			},
			"row_delimiter": {
				"type": "string",
				"defaultValue": "/r,/n, or /r/n"
			},
			"encoding": {
				"type": "string",
				"defaultValue": "\"UTF-8"
			},
			"escape_character": {
				"type": "string",
				"defaultValue": "\\\\"
			},
			"quote_character": {
				"type": "string",
				"defaultValue": "\\"
			},
			"first_row_as_header": {
				"type": "string",
				"defaultValue": "true"
			},
			"null_value": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().adls2_container_name",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().adls2_directory_name",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().adls2_container_name",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"compressionCodec": "snappy",
			"compressionLevel": "Optimal",
			"encodingName": "UTF-7",
			"escapeChar": "\\",
			"firstRowAsHeader": false,
			"quoteChar": "\""
		},
		"schema": []
	}
}